{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "services": "./services",
          "utils": "./utils",
          "components": "./components",
          "styles": "./styles",
          "assets": "./assets"
        },
        "cwd": "babelrc"
      }
    ]
  ]
}
